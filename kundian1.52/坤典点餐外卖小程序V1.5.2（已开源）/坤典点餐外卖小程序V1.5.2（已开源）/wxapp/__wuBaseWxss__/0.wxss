    z-index: 999;
}

.select_container {
    width: 100%;
    background-color: #ffffff;
    position: fixed;
    bottom: 140rpx;
    left: 0;
    z-index: 1001;
}

.select_container_title {
    width: 100%;
    height: 60rpx;
    border-left: 10rpx solid #ffd050;
    display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    font-size: 30rpx;
    background-color: #f4f4f4;
}

.select_container_items {
    width: 100%;
    overflow-y: auto;
    font-size: 32rpx;
}

.select_container_item {
    display: flex;
