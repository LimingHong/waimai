}

.specModels {
    width: 80%;
    background-color: #fff;
    font-size: 30rpx;
    margin: 50% auto 0;
    border-radius: 10rpx;
    position: relative;
    overflow: hidden;
    transition: all 0.3s;
}

.scale0 {
    transform: scale(0,0);
    opacity: 0;
}

.scale1 {
    transform: scale(1,1);
    opacity: 1;
}

.specModels_title {
    width: 100%;
    line-height: 80rpx;
    text-align: center;
    color: #2b2b2b;
    letter-spacing: 3rpx;
}

.spec_containers {
    padding: 10rpx 30rpx;
    box-sizing: border-box;
}

.spec_container {
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 10rpx;
}

.spec_container_item {
    padding: 5rpx 25rpx;
    font-size: 24rpx;
    color: #747474;
    border: 1rpx solid #CBCBCB;
    border-radius: 30rpx;
    margin-right: 15rpx;
    margin-bottom: 15rpx;
}

.spec_footer {
    width: 100%;
    height: 100rpx;
    background-color: #FAFAFA;
    border-top: 1rpx solid #F0F0F0;
    padding: 0rpx 25rpx;
    box-sizing: border-box;
}

.money {
    color: #FF6000;
    font-size: 40rpx;
}

.okBtn {
    padding: 15rpx 25rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    letter-spacing: 3rpx;
    background-color: #FFD050;
