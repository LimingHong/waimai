    justify-content: space-between;
    height: 100rpx;
    padding: 0rpx 20rpx;
}

.select_item_name {
    width: 300rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.select_item_price {
    width: 150rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.select_item_Num {
    width: 180rpx;
    display: flex;
    align-items: center;
    height: 40rpx;
    position: relative;
    margin-right: 0;
}

.addBorderTop {
    border-top: 1rpx solid #f4f4f4;
    padding: 20rpx;
    height: 100%;
