}

.activeDesc {
    width: 550rpx;
    margin-left: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sceneContainer {
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.sceneImg {
    width: 24%;
    height: 170rpx;
