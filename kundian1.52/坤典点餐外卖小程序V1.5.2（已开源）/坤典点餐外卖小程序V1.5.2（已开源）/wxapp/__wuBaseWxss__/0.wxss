.classContainer {
    width: 100%;
    margin: 0rpx auto;
    font-size: 30rpx;
}

.classContainer .view {
    width: 33.3%;
    display: flex;
    justify-content: center;
}

.item {
    position: relative;
    padding: 20rpx 0rpx;
    color: #606060;
}

.item::before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 4rpx;
    background-color: #ffd050;
    transition: all 0.3s ease;
    opacity: 0;
}

.active {
    color: #343434;
    font-weight: bold;
}

.active::before {
    width: 100%;
    left: 0;
    opacity: 1;
    transition: all 0.3s ease;
}

.itemList_container {
    border-top: 1rpx solid #f4f4f4;
    width: 100%;
    display: flex;
    position: fixed;
}

.itemList {
    width: 100%;
    display: flex;
    height: 100%;
    justify-content: space-between;
}

.left {
    width: 24%;
    height: 100%;
    font-size: 28rpx;
}

.left::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.items {
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    padding: 30rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #F1F1F1;
    overflow: hidden;
}

.selectIndex {
    color: #ffd050;
    border-left: 6rpx solid #ffd050;
}

.right {
    width: 76%;
    height: 100%;
    position: relative;
    overflow: hidden;
}

.scrollView {
    width: 100%;
    height: 100%;
}

.scrollView::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.cfa8a7a7 {
    color: #a8a7a7;
}

.curent_item {
