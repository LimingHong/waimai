}

.notReduce {
    width: 50%;
    height: 120rpx;
    border-radius: 15rpx;
    background-color: rgba(0,0,0,0.8);
    position: fixed;
    z-index: 99999;
    top: 40%;
    left: 25%;
    font-size: 34rpx;
    padding: 10rpx 20rpx;
    color: #fff;
    box-sizing: border-box;
    letter-spacing: 3rpx;
    text-align: center;
}

.goods-detail {
    width: 80%;
    height: 650rpx;
    background: white;
    position: fixed;
    top: 22%;
    border-radius: 10rpx;
    margin-left: 10%;
    z-index: 999999;
    font-size: 30rpx;
}

.goods-detail image {
    width: 100%;
    height: 450rpx;
    border-radius: 10rpx;
}

.goods-detail .goods-name {
    margin-left: 20rpx;
    margin-top: 10rpx;
    display: block;
}

.goods-detail .sale-count {
    color: gray;
    margin-left: 20rpx;
    display: block;
}

.goods-detail .price {
    color: red;
    margin-left: 20rpx;
}

.goods-detail .add-cart {
    display: block;
    width: 170rpx;
    height: 50rpx;
    line-height: 50rpx;
    float: right;
    margin-right: 20rpx;
    margin-top: -20rpx;
    background: red;
    color: white;
    text-align: center;
    border-radius: 10rpx;
}

.goods-detail .close-goods-detail {
    width: 20%;
    height: 50rpx;
    text-align: center;
    margin-left: 40%;
    position: relative;
    top: 100rpx;
}

.goods-detail .close-goods-detail image {
    width: 50rpx;
    height: 50rpx;
}

.show {
    display: block;
}

.hide {
    display: none;
}

.mar-btm {
